  com.github.seancorfield/next.jdbc {:mvn/version "1.3.874"}
  org.xerial/sqlite-jdbc {:mvn/version "3.39.2.1"}

  ;; HTSlib
  com.github.samtools/htsjdk {:mvn/version "3.0.5"}

