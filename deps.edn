  com.walmartlabs/lacinia-pedestal                  {:mvn/version "1.2"}
  
  ;; Serialization
  com.taoensso/nippy                                {:mvn/version "3.2.0"}
  digest/digest                                     {:mvn/version "1.4.10"}

  org.apache.kafka/kafka-clients                    {:mvn/version "3.5.0"}

